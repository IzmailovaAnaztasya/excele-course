{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "require-jsdoc":"off",
        "indent":"off",
        "linebreak-style": 0,
        "eol-last": 0,
        "spaced-comment":"off",
        "object-curly-spacing":"off",
        "max-len":"off",
        "semi":"off",
        "no-extra-semi":"off",
        "no-case-declarations": "off"
    },
    "extends": ["eslint:recommended", "google"]
}